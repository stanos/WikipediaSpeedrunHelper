{
    "name": "Wikipedia Speedrun Helper",
    "description": "Wikipedia speedrun helper tool",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
      },
      "permissions": ["storage", "activeTab", "scripting", "tabs"],
      "action": {
        "default_popup": "popup.html"
      }
}